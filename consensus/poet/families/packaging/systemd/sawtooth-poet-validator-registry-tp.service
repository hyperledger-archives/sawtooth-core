[Unit]
Description=Sawtooth TP Validator Registry
After=network.target

[Service]
User=sawtooth
Group=sawtooth
EnvironmentFile=-/etc/default/sawtooth-poet-validator-registry-tp
ExecStart=/usr/bin/poet-validator-registry-tp $SAWTOOTH_POET-VALIDATOR-REGISTRY-TP_ARGS

[Install]
WantedBy=multi-user.target
